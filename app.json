{
  "expo": {
    "name": "Kokoro",
    "slug": "auth",
    "version": "0.0.8",
    "orientation": "portrait",
    "icon": "./assets/app-icons/Assets.xcassets/AppIcon.appiconset/_/1024.png",
    "scheme": "kokoro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cwsbrian.kokoro",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 8,
      "googleServicesFile": "./google-services.json",
      "package": "com.cwsbrian.kokoro",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/app-icons/Assets.xcassets/AppIcon.appiconset/_/1024.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/app-icons/Assets.xcassets/AppIcon.appiconset/_/1024.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": { "backgroundColor": "#000000" }
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "experiments": { "typedRoutes": true, "reactCompiler": true },
    "extra": { "router": {}, "eas": { "projectId": "94ee3271-1480-4037-abdd-e51b8ea5ea65" } }
  }
}
