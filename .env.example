# Copy to .env and fill in. Do not commit .env.
# For EAS Build (preview/production), set these as Environment Variables in
# Expo Dashboard → Project → Secrets, or: npx eas-cli env:create

# Firebase (required for auth)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=

# Google Sign-In (Android/iOS)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=

# Optional
EXPO_PUBLIC_GEMINI_API_KEY=
EXPO_PUBLIC_APP_STORE_URL=
EXPO_PUBLIC_PLAY_STORE_URL=
EXPO_PUBLIC_TERMS_URL=
EXPO_PUBLIC_PRIVACY_URL=
